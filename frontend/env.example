# Frontend Environment Variables
# Copy this file to .env.local for local development

# API Configuration
REACT_APP_API_URL=http://localhost:5000
# Note: /api is automatically appended to REACT_APP_API_URL

# Production API URLs (uncomment for production)
# REACT_APP_API_URL=https://api.hms.srikanthkandi.tech
# REACT_APP_API_BASE_URL=https://api.hms.srikanthkandi.tech/api

# Application Configuration
REACT_APP_NAME=Hospital Management System
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_MOCK_DATA=false

# UI Configuration
REACT_APP_THEME=light
REACT_APP_LANGUAGE=en
REACT_APP_TIMEZONE=UTC

# External Services (if needed)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Development Settings
REACT_APP_DEV_MODE=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_HOT_RELOAD=true

# Build Configuration
GENERATE_SOURCEMAP=false
REACT_APP_BUILD_TIME=2024-01-01T00:00:00Z 